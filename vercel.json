{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "server/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "installCommand": "npm install --production=false",
  "buildCommand": "npx vite build",
  "functions": {
    "server/index.ts": {
      "maxDuration": 30
    }
  }
}